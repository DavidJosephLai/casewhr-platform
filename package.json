{
  "name": "casewhr-platform",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "CaseWhr - Professional Global Freelancing Platform with Trilingual Support (EN/繁體中文/简体中文)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules dist .vite && npm install",
    "clean:win": "if exist node_modules rmdir /s /q node_modules && if exist dist rmdir /s /q dist && if exist .vite rmdir /s /q .vite && npm install",
    "rebuild": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "test:perf": "node scripts/test-performance.js",
    "analyze": "npm run build && npm run test:perf",
    "build:analyze": "vite build --mode production && npm run test:perf",
    "check:performance": "node scripts/check-performance-budget.js",
    "generate:report": "node scripts/generate-performance-report.js",
    "performance:full": "npm run build && npm run check:performance && npm run generate:report"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "lucide-react": "latest",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.1",
    "date-fns": "^3.3.1",
    "react-day-picker": "^8.10.0",
    "@supabase/supabase-js": "^2.39.7",
    "jspdf": "^2.5.1",
    "recharts": "^2.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.2",
    "typescript": "^5.5.3"
  }
}